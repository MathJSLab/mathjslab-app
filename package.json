{
  "name": "mathjslab-calculator",
  "version": "1.0.0",
  "description": "MathJSLab Calculator - An interpreter with language syntax like MATLAB/Octave",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiolindau/mathjslab-calculator.git"
  },
  "keywords": [
    "MathJSLab",
    "mathematics",
    "math",
    "educational",
    "MATLAB",
    "Octave",
    "MathML",
    "interpreter",
    "parser",
    "grammar",
    "yacc",
    "bison",
    "lex",
    "flex",
    "jison"
  ],
  "author": "Sergio Lindau <sergiolindau@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sergiolindau/mathjslab-calculator/issues"
  },
  "homepage": "https://github.com/sergiolindau/mathjslab-calculator#readme",
  "scripts": {
    "git:add:all": "git add .",
    "git:commit:initial": "git commit -m \"Initial Commit\"",
    "git:push": "git push",
    "git:add:commit:push": "npm run git:add:all && npm run git:commit:initial && npm run git:push",
    "update": "npx ncu -u",
    "lint": "eslint \"**/*.ts{,x}\" --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:lint": "npm run format && npm run lint",
    "build-configuration": "node util/build-configuration",
    "assets": "node util/build-assets.js",
    "bundle": "node util/save-bundle.js",
    "prebuild:dev": "npm run build-configuration && npm run assets",
    "build:dev": "set MODE_ENV=development && webpack-dev-server --mode development",
    "prebuild:prod": "npm run build-configuration && npm run assets",
    "build:prod": "set MODE_ENV=production && webpack --mode production",
    "postbuild:prod": "npm run bundle",
    "build": "npm run build:prod",
    "clean": "rimraf dist src/first-example.json src/build-configuration.json mathjslab-calc.min.js"
  },
  "dependencies": {
    "basic-dom-helper": "^1.0.6",
    "mathjslab": "^1.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@types/webpack": "^5.28.3",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "css-loader": "^6.8.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "html-webpack-plugin": "^5.5.3",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  }
}
