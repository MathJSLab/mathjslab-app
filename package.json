{
  "name": "mathjslab-app",
  "version": "1.5.1",
  "description": "MathJSLab - An interpreter with language syntax like MATLABÂ®/Octave, ISBN 978-65-00-82338-7. A demo application from the mathjslab npm package, ISBN 978-65-00-84828-1.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathJSLab/mathjslab-app.git"
  },
  "keywords": [
    "MathJSLab",
    "Mathematics",
    "Educational",
    "MATLAB",
    "Octave",
    "MathML",
    "ANTLR",
    "ANTLR4",
    "math",
    "lexer",
    "parser",
    "grammar",
    "interpreter"
  ],
  "author": "Sergio Lindau <sergiolindau@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MathJSLab/mathjslab-app/issues"
  },
  "homepage": "https://mathjslab.com/",
  "scripts": {
    "clean:package-lock": "node script/clean-package-lock.cjs",
    "clean:build": "rimraf dist src/first-example.json src/build-configuration.json mathjslab-app.min.js",
    "clean:android": "rimraf android",
    "clean:build:android": "npm run clean:build && npm run clean:android",
    "clean:download-resources": "node script/download-files.cjs clean && rimraf script/download-files.cjs download.config.json",
    "clean": "npm run clean:build:android",
    "clean:all": "npm run clean && npm run clean:package-lock",
    "update": "npx npm-check-updates -u && npm install",
    "git:add": "git add .",
    "git:commit": "git commit -m \"Initial Commit\"",
    "git:push": "git push",
    "git:all": "npm run git:add && npm run git:commit && npm run git:push",
    "format": "prettier --write **/*.{ts,tsx,js,jsx,cjs,mjs}",
    "lint": "eslint --fix",
    "format:lint": "npm run format && npm run lint",
    "download-resources": "curl -k -o ./script/download-files.cjs https://raw.githubusercontent.com/MathJSLab/.github/refs/heads/main/script/helper/download-files.cjs && curl -k -o ./download.config.json https://raw.githubusercontent.com/MathJSLab/.github/refs/heads/main/resource/mathjslab-app/download.config.json && node script/download-files.cjs",
    "build-configuration": "node script/build-configuration.js",
    "set-sitemap": "node script/set-sitemap.js",
    "build-assets": "node script/build-assets.js",
    "save-bundle": "node script/save-bundle.js",
    "prebuild:dev": "npm run download-resources && npm run build-configuration && npm run set-sitemap && npm run build-assets",
    "build:dev": "tsx --require tsconfig-paths/register ./node_modules/webpack-dev-server/bin/webpack-dev-server --mode development",
    "prebuild:debug": "set DEBUG=true && npm run build-configuration && npm run build-assets",
    "build:debug": "tsx --require tsconfig-paths/register ./node_modules/webpack-dev-server/bin/webpack-dev-server --mode development",
    "prebuild:prod": "npm run prebuild:dev",
    "build:prod": "tsx --require tsconfig-paths/register ./node_modules/webpack/bin/webpack --mode production",
    "postbuild:prod": "npm run save-bundle",
    "build": "npm run build:prod",
    "add:android": "npx cap add android",
    "sync:android": "npx cap sync android",
    "build:add:android": "npm run build:prod && npm run add:android",
    "open:android": "npx cap open android",
    "build:all": "npm run clean && npm run update && npm run build:prod && npm run add:android",
    "do:all": "npm run clean:all && npm run update && npm run format:lint && npm run build:prod && npm run git:all"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "devDependencies": {
    "@capacitor/cli": "^6.2.0",
    "@types/dotenv-webpack": "^7.0.8",
    "@types/node": "^22.10.1",
    "@types/plotly.js": "^2.35.1",
    "@types/webpack": "^5.28.5",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@typescript-eslint/eslint-plugin": "^8.16.0",
    "@typescript-eslint/parser": "^8.16.0",
    "ajv": "^8.17.1",
    "css-loader": "^7.1.2",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^12.0.1",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.16.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "html-webpack-plugin": "^5.6.3",
    "lint-staged": "^15.2.10",
    "mini-css-extract-plugin": "^2.9.2",
    "prettier": "^3.4.1",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "webpack": "^5.96.1",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "@capacitor/android": "^6.2.0",
    "@capacitor/core": "^6.2.0",
    "mathjslab": "^1.6.2"
  }
}
