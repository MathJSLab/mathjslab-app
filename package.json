{
  "name": "mathjslab-app",
  "version": "1.5.6",
  "description": "MathJSLab - An interpreter with language syntax like MATLABÂ®/Octave, ISBN 978-65-00-82338-7. A demo application from the mathjslab npm package, ISBN 978-65-00-84828-1.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathJSLab/mathjslab-app.git"
  },
  "keywords": [
    "MathJSLab",
    "Mathematics",
    "Educational",
    "MATLAB",
    "Octave",
    "MathML",
    "ANTLR",
    "ANTLR4",
    "math",
    "lexer",
    "parser",
    "grammar",
    "interpreter"
  ],
  "author": "MathJSLab <mathjslab@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MathJSLab/mathjslab-app/issues"
  },
  "homepage": "https://mathjslab.com/",
  "type": "module",
  "scripts": {
    "clean:package-lock": "node script/helper/clean-package-lock.cjs",
    "clean:script": "rimraf script/build",
    "clean:build": "rimraf dist report src/first-example.json src/build-configuration.json src/styles/constants.json",
    "clean:android": "rimraf android",
    "clean:script:build:android": "npm run clean:script && npm run clean:build && npm run clean:android",
    "clean:download-resources": "node script/helper/download-files.cjs clean && rimraf script/helper/download-files.cjs download.config.json",
    "clean": "npm run clean:script:build:android",
    "clean:all": "npm run clean && npm run remove:webapp-dep && npm run clean:package-lock && npm run clean:download-resources",
    "download-resources": "curl -k -o ./script/helper/download-files.cjs https://raw.githubusercontent.com/MathJSLab/.github/refs/heads/main/script/helper/download-files.cjs && curl -k -o ./download.config.json https://raw.githubusercontent.com/MathJSLab/.github/refs/heads/main/resource/mathjslab-app/download.config.json && node script/helper/download-files.cjs",
    "preinstall:android-dep": "npm install @capacitor/core && npm install --save-dev @capacitor/cli",
    "install:android-dep": "npm install @capacitor/android",
    "remove:android-dep": "npm remove @capacitor/android",
    "install:webapp-dep": "npm run install:android-dep",
    "remove:webapp-dep": "npm run remove:android-dep && npm remove @capacitor/core @capacitor/cli",
    "update": "npx npm-check-updates -u && npm install && npm run install:webapp-dep && npm run download-resources && npm run clean && npm run build:script",
    "git:add": "git add .",
    "git:commit": "node script/helper/git-commit.cjs",
    "git:push": "git push",
    "git:all": "npm run git:add && npm run git:commit && npm run git:push",
    "format": "prettier --plugin=@prettier/plugin-xml --write **/*.{ts,tsx,js,jsx,cjs,mjs,css,scss,html,htm,json,jsonc,yml,yaml,xml,svg}",
    "lint": "eslint --fix",
    "format:lint": "npm run format && npm run lint",
    "build:script": "tsc -p script/tsconfig.json",
    "build-assets": "node script/build-assets.cjs",
    "build:development": "tsx --require tsconfig-paths/register ./node_modules/webpack-dev-server/bin/webpack-dev-server --mode development",
    "prebuild:dev": "npm run build-assets && node eleventy.build.mjs",
    "build:dev": "npm run build:development",
    "prebuild:debug": "cross-env DEBUG=true npm run prebuild:dev",
    "build:debug": "npm run build:development",
    "prebuild:prod": "npm run prebuild:dev",
    "build:prod": "tsx --require tsconfig-paths/register ./node_modules/webpack/bin/webpack --mode production",
    "build": "npm run build:prod",
    "add:android": "npx cap add android",
    "sync:android": "npx cap sync android",
    "build:add:android": "npm run build:prod && npm run add:android",
    "open:android": "npx cap open android",
    "build:all": "npm run clean && npm run update && npm run build:prod && npm run add:android",
    "do:all": "npm run clean:all && npm run update && npm run format:lint && npm run build:prod && npm run git:all"
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^6.0.1",
    "@capacitor/cli": "^7.1.0",
    "@prettier/plugin-xml": "^3.4.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/node": "^22.13.13",
    "@types/plotly.js": "^2.35.2",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@typescript-eslint/eslint-plugin": "^8.28.0",
    "@typescript-eslint/parser": "^8.28.0",
    "ajv": "^8.17.1",
    "chalk": "^5.4.1",
    "coffeescript": "^2.7.0",
    "cross-env": "^7.0.3",
    "cson": "^8.4.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.23.0",
    "eslint-config-prettier": "^10.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.4",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.3",
    "json5": "^2.2.3",
    "mini-css-extract-plugin": "^2.9.2",
    "png-to-ico": "^2.1.8",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "sass": "^1.86.0",
    "sass-loader": "^16.0.5",
    "sass-to-string": "^1.6.4",
    "smol-toml": "^1.3.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "uuid": "^11.1.0",
    "webpack": "^5.98.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.0",
    "yaml": "^2.7.0"
  },
  "dependencies": {
    "@capacitor/android": "^7.1.0",
    "@capacitor/core": "^7.1.0",
    "mathjslab": "^1.7.2"
  }
}
