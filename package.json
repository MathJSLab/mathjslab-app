{
  "name": "mathjslab-app",
  "version": "1.6.1",
  "description": "MathJSLab - An interpreter with language syntax like MATLABÂ®/Octave, ISBN 978-65-00-82338-7. A demo application from the mathjslab npm package, ISBN 978-65-00-84828-1.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathJSLab/mathjslab-app.git"
  },
  "keywords": [
    "MathJSLab",
    "Mathematics",
    "Educational",
    "MATLAB",
    "Octave",
    "MathML",
    "ANTLR",
    "ANTLR4",
    "math",
    "lexer",
    "parser",
    "grammar",
    "interpreter"
  ],
  "author": "MathJSLab <mathjslab@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MathJSLab/mathjslab-app/issues"
  },
  "homepage": "https://mathjslab.com/",
  "type": "module",
  "scripts": {
    "clean:package-lock": "node script/helper/clean-package-lock.cjs",
    "clean:build": "shx rm -rf dist report sitemap.xml mathjslab-en.bib mathjslab-pt.bib mathjslab-es.bib src/first-example.json src/build-configuration.json src/styles/constants.json",
    "clean:script": "shx rm -rf script/build",
    "clean:android": "shx rm -rf android",
    "clean:webapp": "npm run clean:android",
    "clean:build:script:webapp": "npm run clean:build && npm run clean:script && npm run clean:webapp",
    "clean:download": "node script/helper/download-files.cjs clean && npx shx rm -rf script/helper/download-files.cjs download.config.json",
    "clean": "npm run clean:build:script:webapp",
    "clean:all": "npm run clean && npm run clean:package-lock && npm run clean:download",
    "set:prettierrc": "node -e \"import fs from 'fs';const f='.prettierrc';let j=JSON.parse(fs.readFileSync(f));j.overrides.filter((c)=>c.files==='*.md')[0].options.printWidth=79;fs.writeFileSync(f,JSON.stringify(j,null,2));\"",
    "predownload": "curl -k -o ./script/helper/download-files.cjs https://raw.githubusercontent.com/MathJSLab/.github/refs/heads/main/script/helper/download-files.cjs && curl -k -o ./download.config.json https://raw.githubusercontent.com/MathJSLab/.github/refs/heads/main/resource/mathjslab-app/download.config.json",
    "download": "node script/helper/download-files.cjs",
    "postdownload": "npm run set:prettierrc",
    "install:capacitor": "npm install @capacitor/core && npm install --save-dev @capacitor/cli",
    "remove:capacitor": "npm remove @capacitor/core @capacitor/cli",
    "install:android": "npm install @capacitor/android",
    "remove:android": "npm remove @capacitor/android",
    "install:webapp": "npm run install:capacitor && npm run install:android",
    "remove:webapp": "npm run remove:android && npm run remove:capacitor",
    "update": "npx npm-check-updates -u && npm install && npm run download && npm run clean && npm run build:script",
    "clean:all:update": "npm run clean:all && npm run update",
    "git:add": "git add .",
    "git:commit": "node script/helper/git-commit.cjs",
    "git:push": "git push",
    "git:all": "npm run git:add && npm run git:commit && npm run git:push",
    "tsx": "tsx --require tsconfig-paths/register",
    "preformat": "npm run tsx script/helper/prettier-math-protect.ts -- --protect . --ignore **/node_modules/** dist/**",
    "format": "prettier --plugin=@prettier/plugin-xml --write **/*.{ts,tsx,js,jsx,cjs,mjs,css,scss,html,htm,json,jsonc,yml,yaml,xml,svg,md}",
    "postformat": "npm run tsx script/helper/prettier-math-protect.ts -- --restore . --ignore **/node_modules/** dist/**",
    "lint": "eslint --fix",
    "format:lint": "npm run format && npm run lint",
    "build:script": "tsc -p script/tsconfig.json",
    "build-assets": "node script/build-assets.cjs",
    "build:development": "tsx --require tsconfig-paths/register ./node_modules/webpack-dev-server/bin/webpack-dev-server --mode development",
    "prebuild:dev": "node eleventy.build.mjs && npm run build-assets",
    "build:dev": "npm run build:development",
    "prebuild:debug": "cross-env DEBUG=true npm run prebuild:dev",
    "build:debug": "npm run build:development",
    "prebuild": "npm run prebuild:dev",
    "build:prod": "tsx --require tsconfig-paths/register ./node_modules/webpack/bin/webpack --mode production",
    "build": "npm run build:prod",
    "add:android": "npx cap add android",
    "sync:android": "npx cap sync android",
    "build:add:android": "npm run build && npm run add:android",
    "build:sync:android": "npm run build && npm run sync:android",
    "open:android": "npx cap open android",
    "build:add:open:android": "npm run build:add:android && npm run open:android",
    "build:sync:open:android": "npm run build:sync:android && npm run open:android",
    "build:all": "npm run clean && npm run update && npm run build:add:android",
    "do:all": "npm run clean:all && npm run update && npm run format:lint && npm run build && npm run git:all && echo script \"do:all\" finished."
  },
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-img": "^6.0.4",
    "@prettier/plugin-xml": "^3.4.2",
    "@resvg/resvg-js": "^2.6.2",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/micromatch": "^4.0.9",
    "@types/node": "^24.8.0",
    "@types/plotly.js": "^3.0.7",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@types/wicg-file-system-access": "^2023.10.7",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "ajv": "^8.17.1",
    "chalk": "^5.6.2",
    "coffeescript": "^2.7.0",
    "cross-env": "^10.1.0",
    "cson": "^8.4.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "html-loader": "^5.1.0",
    "html-webpack-plugin": "^5.6.4",
    "json5": "^2.2.3",
    "micromatch": "^4.0.8",
    "mini-css-extract-plugin": "^2.9.4",
    "png-to-ico": "^3.0.1",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "sass-to-string": "^1.6.4",
    "shx": "^0.4.0",
    "smol-toml": "^1.4.2",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "uuid": "^13.0.0",
    "webpack": "^5.102.1",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "yaml": "^2.8.1"
  },
  "dependencies": {
    "mathjslab": "^1.8.0"
  }
}
