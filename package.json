{
  "name": "mathjslab-calculator",
  "version": "0.0.1",
  "description": "MathJSLab Calculator - Interpreter with language syntax like MATLAB/Octave",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sergiolindau/calculator.git"
  },
  "keywords": [
    "MathJSLab",
    "MATLAB",
    "Octave",
    "interpreter",
    "parser",
    "yacc",
    "bison",
    "lex",
    "flex",
    "jison"
  ],
  "author": "Sergio Lindau <sergiolindau@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sergiolindau/calculator/issues"
  },
  "homepage": "https://github.com/sergiolindau/calculator#readme",
  "scripts": {
    "git:add:all": "git add .",
    "git:commit:initial": "git commit -m \"Initial Commit\"",
    "git:push": "git push",
    "git:add:commit:push": "npm run git:add:all && npm run git:commit:initial && npm run git:push",
    "update": "npx ncu -u",
    "lint": "eslint \"**/*.ts{,x}\" --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:lint": "npm run format && npm run lint",
    "tscbuild": "tsc -p tsconfig.build.json",
    "build:dev": "webpack-dev-server --mode development",
    "build:prod": "webpack --mode production",
    "clean": "rimraf dist",
    "test:decimal": "ts-node-dev --respawn --transpile-only test/decimal-test.ts"
  },
  "dependencies": {
    "basic-dom-helper": "file:../basic-dom-helper",
    "mathjslab": "file:../mathjslab"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/webpack": "^5.28.1",
    "@types/webpack-dev-server": "^4.7.2",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "html-webpack-plugin": "^5.5.3",
    "lint-staged": "^14.0.0",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "sass": "^1.65.1",
    "sass-loader": "^13.3.2",
    "style-loader": "^3.3.3",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  }
}
