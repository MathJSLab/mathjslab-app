{
  "name": "mathjslab-calculator",
  "version": "1.3.0",
  "description": "MathJSLab - An interpreter with language syntax like MATLABÂ®/Octave, ISBN 978-65-00-82338-7. A demo application from the mathjslab npm package, ISBN 978-65-00-84828-1.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MathJSLab/mathjslab-calculator.git"
  },
  "keywords": [
    "MathJSLab",
    "Mathematics",
    "Educational",
    "MATLAB",
    "Octave",
    "MathML",
    "ANTLR",
    "ANTLR4",
    "math",
    "lexer",
    "parser",
    "grammar",
    "interpreter"
  ],
  "author": "Sergio Lindau <sergiolindau@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/MathJSLab/mathjslab-calculator/issues"
  },
  "homepage": "https://mathjslab.com/",
  "scripts": {
    "clean:package-lock": "node script/clean-package-lock.cjs",
    "clean": "rimraf dist src/first-example.json src/build-configuration.json mathjslab-calculator.min.js",
    "clean:all": "npm run clean && npm run clean:package-lock",
    "update": "npx ncu -u && npm install",
    "git:add": "git add .",
    "git:commit": "git commit -m \"Initial Commit\"",
    "git:push": "git push",
    "git:add:commit:push": "npm run git:add && npm run git:commit && npm run git:push",
    "lint": "eslint \"**/*.ts{,x}\" --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:lint": "npm run format && npm run lint",
    "build-configuration": "node script/build-configuration.js",
    "set-sitemap": "node script/set-sitemap.js",
    "build-assets": "node script/build-assets.js",
    "save-bundle": "node script/save-bundle.js",
    "prebuild:dev": "npm run build-configuration && npm run set-sitemap && npm run build-assets",
    "build:dev": "tsx --require tsconfig-paths/register ./node_modules/webpack-dev-server/bin/webpack-dev-server --mode development",
    "prebuild:debug": "set DEBUG=true && npm run build-configuration && npm run build-assets",
    "build:debug": "tsx --require tsconfig-paths/register ./node_modules/webpack-dev-server/bin/webpack-dev-server --mode development",
    "prebuild:prod": "npm run prebuild:dev",
    "build:prod": "tsx --require tsconfig-paths/register ./node_modules/webpack/bin/webpack --mode production",
    "postbuild:prod": "npm run save-bundle",
    "build": "npm run build:prod",
    "do:all": "npm run clean && npm run update && npm run format:lint && npm run build:prod && npm run git:add:commit:push"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "devDependencies": {
    "@types/node": "^22.7.4",
    "@types/plotly.js": "^2.33.4",
    "@types/webpack": "^5.28.5",
    "@types/wicg-file-system-access": "^2023.10.5",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.7.0",
    "ajv": "^8.17.1",
    "css-loader": "^7.1.2",
    "eslint": "^9.11.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.30.0",
    "eslint-plugin-prettier": "^5.2.1",
    "html-webpack-plugin": "^5.6.0",
    "lint-staged": "^15.2.10",
    "mini-css-extract-plugin": "^2.9.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "mathjslab": "^1.6.0"
  }
}
